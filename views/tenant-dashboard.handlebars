<div class="col-10 main-container">
    <h1 class="dashboard-title">Hello, <br> put data!</h1>
    <div class="row justify-content-start">
        <div class="table-wrapper shadow col-5">
            <div class="table-title">Your Information</div>
            <div class="row table-info">
                <div class="col-6">
                    <form action="">
                        <fieldset disabled="disabled">
                            <div class="form-group">
                                <label for="disabledTextInput">First Name</label>
                                <input type="text" id="disabledTextInput" class="form-control" placeholder="put in data">
                            </div>
                            <div class="form-group">
                                <label for="disabledTextInput">Last Name</label>
                                <input type="text" id="disabledTextInput" class="form-control" placeholder="put in data">
                            </div>
                            <div class="form-group">
                                <label for="disabledTextInput">Email</label>
                                <input type="text" id="disabledTextInput" class="form-control" placeholder="put in data">
                            </div>
                        </fieldset>
                    </form>
                </div>
                <div class="col-6">
                    <form action="">
                        <fieldset disabled="disabled">
                            <div class="form-group">
                                <label for="disabledTextInput">Complex</label>
                                <input type="text" id="disabledTextInput" class="form-control" placeholder="put in data">
                            </div>
                            <div class="form-group">
                                <label for="disabledTextInput">Apartment</label>
                                <input type="text" id="disabledTextInput" class="form-control" placeholder="put in data">
                            </div>
                            <div class="form-group">
                                <label for="disabledTextInput">Rent</label>
                                <input type="text" id="disabledTextInput" class="form-control" placeholder="put in data">
                            </div>
                        </fieldset>
                    </form>
                </div>
            </div>
            <a href="#" data-toggle="modal" data-target="#update-info" class="update-button">Update Info</a>
        </div>
        <div class="col-6">
            <div class="col-12 service-wrapper shadow">
                <div class="table-title">Request Maintenance</div>
                <p class="table-desc">Please contact us if you need any maintenance on:</p>
                <div class="table-info">
                    <ul>
                        <li>Physical damages</li>
                        <li>Appliances</li>
                    </ul>
                </div>
                <a href="#" id ="maintenance-begin-request" class="update-button">Begin Request</a>
            </div>
            <div class="col-12 service-wrapper shadow">
                <div class="table-title">Update Pet Status</div>
                <p class="table-desc">Please update if you get or lose a pet</p>
                <a href="#" class="update-button">Update</a>
            </div>
        </div>
    </div>
</div>


<!--modal-->
<div class="modal fade" id="update-info" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Update Info</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form action="">
                    <div class="left-form">
                        <label for="firstName">First Name:</label>
                        <input type="text" class="firstName" placeholder="put in data">
                    </div>
                    <div class="right-form">
                        <label for="lastName">Last Name:</label>
                        <input type="text" class="lastName" placeholder="Jobs">
                    </div>
                    <label for="email">Email:</label>
                    <input type="text" class="email" placeholder="email">
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="form-button">Submit</button>
            </div>
        </div>
    </div>
</div>

<div>
    <button type="button" class="update-button" id="tenantlogout">Log Out</button>
</div>

